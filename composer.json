{
	"name": "flarum-lang/khmer",
	"type": "flarum-extension",
	"description": "Khmer language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/khmer",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"km-locale",
		"khmer-locale",
		"khmer"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/khmer/issues",
		"source": "https://github.com/flarum-lang/khmer"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Khmer",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "km",
			"title": "Khmer"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
